{
  "name": "nd-in-tech",
  "version": "0.0.2",
  "description": "Website for Neurodiversity in Tech",
  "private": true,
  "keywords": [
    "website"
  ],
  "license": "MIT",
  "author": "Neurodiversity in Tech",
  "repository": {
    "type": "git",
    "url": "git@github.com:nditcommunity/ndit-website.git"
  },
  "scripts": {
    "build": "npx @11ty/eleventy",
    "clean": "rm -rf _site",
    "debug": "DEBUG=* npx @11ty/eleventy",
    "dev": "npx @11ty/eleventy --serve",
    "dry-run": "npx @11ty/eleventy --dryrun",
    "build:eleventy:prod": "ELEVENTY_ENV=production eleventy",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@shopify/prettier-plugin-liquid": "^1.4.4",
    "prettier": "^3.2.5"
  }
}
